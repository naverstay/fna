mixin order_form(id_mod)
    .form_cell.cell_v1
        label.hidden_input(for= id_mod + 'order_name') Имя и фамилия
        .input_w: input.f_input.input_v1(placeholder='Имя и фамилия', id = id_mod + 'order_name')
    
    .form_row.cell_2
        .form_cell.cell_v1
            label.hidden_input(for= id_mod + 'order_phone') Телефон
            .input_w: input.f_input.input_v1(placeholder='Телефон', id = id_mod + 'order_phone')

        .form_cell.cell_v1
            label.hidden_input(for= id_mod + 'order_email') Электронная почта
            .input_w: input.f_input.input_v1(placeholder='Электронная почта', id = id_mod + 'order_email')

